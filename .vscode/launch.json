    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Kitchen No Fluents: Normal",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/experiments/kitchen_no_fluents/run.py",
            "console": "integratedTerminal",
            "env": {
                "ORACLE": "False",
                "DEFAULT_UNIQUE": "False"
            },
            "args": ["--mode", "save"]
        },
        {
            "name": "Kitchen No Fluents: Oracle",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/experiments/kitchen_no_fluents/run.py",
            "console": "integratedTerminal",
            "env": {
                "ORACLE": "True",
                "DEFAULT_UNIQUE": "True"
            },
            "args": ["--mode", "oracle"]
        }
    ]
}