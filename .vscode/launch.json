            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Kitchen",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/experiments/kitchen_no_fluents/run.py",
            "console": "integratedTerminal"
        },
        {
            "name": "run main.py test",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/learning/gnn/main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "--datafile",
                "learning/data/experiments/kitchen_diffclasses.json",
                "--model-home", "model_files/kitchen_diffclasses",
                "--model",
                "hyper",
                "--use-problem-graph",
                "--batch-size",
                "128",
                "--epoch-size",
                "1280",
                "--lr",
                "0.001",
                "--num-preprocessors",
                "8",
                "--save-every",
                "100",
                "--epochs",
                "1000",
            ],
        },
    ]
